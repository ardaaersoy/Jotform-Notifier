{
    "name": "JotForm Notifier",
    "version": "1.4",
    "description": "Get JotForm form submission notifications on your browser instantly.",
    "permissions": [
        "activeTab",
        "declarativeContent",
        "storage",
        "http://intern-2019.jotform.io/"
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "400": "jotform-icon.png"
        }
    },
    "options_page": "options.html",
    "icons": {
        "400": "jotform-icon.png"
    },
    "manifest_version": 2
}